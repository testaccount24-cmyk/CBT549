/******************************* REXX ********************************/
TRACE O
ADDRESS TSO

PANELID = "XUNITS"
UNITSOUT_DSN = "'"SYSVAR(SYSUID)".EDT.LIST'"
MSGSTAT = MSG(OFF)
"DEL" UNITSOUT_DSN
"ALLOC FI(UNITSOUT) DA("UNITSOUT_DSN") UNIT(SYSALLDA),
       SPACE(1 1) TRACKS"

ADDRESS ISPEXEC
DISPLAY:
"DISPLAY PANEL("PANELID")"
IF RC = 8 THEN SIGNAL EXIT

ADDRESS ISPEXEC
"SELECT PGM(UNITNAME) PARM("NAMES")"
IF RC Â¬= 0 THEN SIGNAL EXIT

"BROWSE DATASET("UNITSOUT_DSN")"
SIGNAL DISPLAY

EXIT:
ADDRESS TSO
"FREE FI(UNITSOUT)"
"DEL" UNITSOUT_DSN
MSGSTAT = MSG(MSGSTAT)
EXIT(0)
